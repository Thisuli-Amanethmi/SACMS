      </ImageView>
      <Button fx:id="btnBack" layoutX="50.0" layoutY="43.0" mnemonicParsing="false" onAction="#loadManageClub" prefHeight="35.0" prefWidth="35.0" style="-fx-background-color: white;" text="Button" />
      <ImageView fitHeight="35.0" fitWidth="35.0" layoutX="50.0" layoutY="43.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@BackButton.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtSearchClubName" layoutX="260.0" layoutY="140.0" prefHeight="26.0" prefWidth="281.0" promptText="Search Club Name" />
      <Button fx:id="btnSearch" layoutX="555.0" layoutY="140.0" mnemonicParsing="false" onAction="#searchClubName" prefHeight="26.0" prefWidth="35.0" style="-fx-background-color: white;" text="Button" />
