<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sacms_grp30.ClubAdvisorCheckMembersController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="800.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
