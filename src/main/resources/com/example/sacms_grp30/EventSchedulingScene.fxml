<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sacms_grp30.EventSchedulingController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@EventSchedulingScene.png" />
         </image>
      </ImageView>
      <Button fx:id="EventButton" layoutX="499.0" layoutY="230.0" mnemonicParsing="false" onAction="#handleEventButton" prefHeight="32.0" prefWidth="201.0" style="-fx-background-color: black;" text="SCHEDULE EVENTS" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
      <Button fx:id="MeetingButton" layoutX="499.0" layoutY="388.0" mnemonicParsing="false" onAction="#handleMeetingButton" prefHeight="32.0" prefWidth="201.0" style="-fx-background-color: black;" text="SCHEDULE MEETINGS" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="0.0" />
   </children>
</AnchorPane>
